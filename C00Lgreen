local targetTextureID = "126374845097716"
local rbxAssetURL = "rbxassetid://" .. targetTextureID

-- –°–æ—Ö—Ä–∞–Ω–µ–Ω–∏–µ –º–µ–Ω—é –ø–æ—Å–ª–µ —Ä–µ—Å–µ—Ç–∞
if not _G.DarkGPTMenu then
    _G.DarkGPTMenu = true
    
    -- ========== –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò ========== --

    -- 1. –ò—Å–ø—Ä–∞–≤–ª–µ–Ω–Ω—ã–π Caducus execution
    local function executeCaducusScript()
        pcall(function()
            -- –ü—Ä—è–º–æ–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –±–µ–∑ –ª–∏—à–Ω–∏—Ö –ø—Ä–æ–≤–µ—Ä–æ–∫
            loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-FE-Caducus-The-fallen-god-REQUIRES-REANIMATION-TO-WORK-47600", true))()
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ Caducus injected", "All")
        end)
    end

    -- 2. –£–ª—É—á—à–µ–Ω–Ω—ã–π Backdoor Scanner
    local function checkBackdoor()
        pcall(function()
            local backdoorFound = false
            local scanLocations = {
                game:GetService("ReplicatedStorage"),
                game:GetService("Workspace"), 
                game:GetService("Lighting"),
                game:GetService("ServerScriptService"),
                game:GetService("ServerStorage")
            }
            
            local backdoorKeywords = {
                "Backdoor", "Admin", "Script", "Execute", "Cmd", "Command",
                "System", "Control", "Panel", "Hub", "Menu", "GUI"
            }
            
            -- –°–∫–∞–Ω–∏—Ä—É–µ–º –≤—Å–µ –ª–æ–∫–∞—Ü–∏–∏
            for _, location in ipairs(scanLocations) do
                for _, keyword in ipairs(backdoorKeywords) do
                    if location:FindFirstChild(keyword) then
                        backdoorFound = true
                        break
                    end
                end
                if backdoorFound then break end
            end
            
            -- –û–±–Ω–æ–≤–ª—è–µ–º —Ç–µ–∫—Å—Ç –≤ —Å–∫–∞–Ω–µ—Ä–µ
            if game:GetService("CoreGui"):FindFirstChild("BackdoorScanner") then
                local statusText = game:GetService("CoreGui").BackdoorScanner.MainFrame.StatusText
                if backdoorFound then
                    statusText.Text = "BACKDOOR: ‚úÖ HAVE"
                    statusText.TextColor3 = Color3.fromRGB(0, 255, 0)
                else
                    statusText.Text = "BACKDOOR: ‚ùå NONE"
                    statusText.TextColor3 = Color3.fromRGB(255, 0, 0)
                end
            end
            
            -- –ß–∞—Ç-—Å–æ–æ–±—â–µ–Ω–∏–µ
            if backdoorFound then
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚úÖ Backdoor: FOUND", "All")
            else
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("‚ùå Backdoor: NOT FOUND", "All")
            end
        end)
    end

    -- 3. –û—Å—Ç–∞–ª—å–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏
    local function changeFullUsername()
        pcall(function()
            game.Players.LocalPlayer.DisplayName = "c00lgreen"
        end)
    end

    local function changePlayerSkin()
        pcall(function()
            local character = game.Players.LocalPlayer.Character
            if character then
                for _, part in ipairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.Color = Color3.fromRGB(0, 255, 0)
                    end
                end
            end
        end)
    end

    local function changeSky()
        pcall(function()
            local lighting = game:GetService("Lighting")
            local sky = lighting:FindFirstChildOfClass("Sky") or Instance.new("Sky")
            sky.Parent = lighting
            sky.SkyboxBk = rbxAssetURL
            sky.SkyboxFt = rbxAssetURL
            sky.SkyboxLf = rbxAssetURL
            sky.SkyboxRt = rbxAssetURL
        end)
    end

    local function switchBodyType()
        pcall(function()
            local humanoid = game.Players.LocalPlayer.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.RigType = humanoid.RigType == Enum.HumanoidRigType.R6 and Enum.HumanoidRigType.R15 or Enum.HumanoidRigType.R6
            end
        end)
    end

    -- ========== –°–û–ó–î–ê–ù–ò–ï –û–°–ù–û–í–ù–û–ì–û –ú–ï–ù–Æ ========== --

    local function createMainMenu()
        if game:GetService("CoreGui"):FindFirstChild("DarkGPT_FE_Menu") then
            game:GetService("CoreGui").DarkGPT_FE_Menu:Destroy()
        end

        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "DarkGPT_FE_Menu"
        ScreenGui.Parent = game:GetService("CoreGui")
        ScreenGui.ResetOnSpawn = false

        local MainFrame = Instance.new("Frame")
        MainFrame.Size = UDim2.new(0.45, 0, 0.75, 0)
        MainFrame.Position = UDim2.new(0.02, 0, 0.15, 0)
        MainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
        MainFrame.BorderColor3 = Color3.fromRGB(255, 0, 0)
        MainFrame.BorderSizePixel = 2
        MainFrame.Active = true
        MainFrame.Parent = ScreenGui

        local Title = Instance.new("TextLabel")
        Title.Size = UDim2.new(1, 0, 0.07, 0)
        Title.Text = "‚ò†Ô∏è DARKGPT MENU"
        Title.TextColor3 = Color3.fromRGB(255, 0, 0)
        Title.BackgroundTransparency = 1
        Title.Font = Enum.Font.SourceSansBold
        Title.TextSize = 14
        Title.Parent = MainFrame

        -- –ö–Ω–æ–ø–∫–∏ –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –º–µ–Ω—é
        local buttons = {
            {Text = "‚ö° CADUCUS FE", Pos = 0.08, Color = Color3.fromRGB(139, 0, 139), Func = executeCaducusScript},
            {Text = "üöÄ CHANGE NAME", Pos = 0.16, Color = Color3.fromRGB(255, 50, 50), Func = changeFullUsername},
            {Text = "üé≠ CHANGE SKIN", Pos = 0.24, Color = Color3.fromRGB(0, 150, 255), Func = changePlayerSkin},
            {Text = "üåå CHANGE SKY", Pos = 0.32, Color = Color3.fromRGB(100, 0, 200), Func = changeSky},
            {Text = "üîÅ BODY TYPE", Pos = 0.40, Color = Color3.fromRGB(200, 100, 0), Func = switchBodyType},
            {Text = "üîÑ SCAN NOW", Pos = 0.48, Color = Color3.fromRGB(255, 165, 0), Func = checkBackdoor}
        }

        for _, btn in ipairs(buttons) do
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0.9, 0, 0.1, 0)
            button.Position = UDim2.new(0.05, 0, btn.Pos, 0)
            button.Text = btn.Text
            button.BackgroundColor3 = btn.Color
            button.TextColor3 = Color3.fromRGB(255, 255, 255)
            button.Font = Enum.Font.SourceSansBold
            button.TextSize = 11
            button.BorderSizePixel = 1
            button.BorderColor3 = Color3.fromRGB(50, 50, 50)
            button.Parent = MainFrame
            button.MouseButton1Click:Connect(btn.Func)
        end

        -- –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
        local CloseButton = Instance.new("TextButton")
        CloseButton.Size = UDim2.new(0.12, 0, 0.07, 0)
        CloseButton.Position = UDim2.new(0.84, 0, 0.005, 0)
        CloseButton.Text = "X"
        CloseButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        CloseButton.Font = Enum.Font.SourceSansBold
        CloseButton.TextSize = 12
        CloseButton.Parent = MainFrame
        CloseButton.MouseButton1Click:Connect(function()
            ScreenGui:Destroy()
        end)

        -- –°–∏—Å—Ç–µ–º–∞ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è –æ—Å–Ω–æ–≤–Ω–æ–≥–æ –º–µ–Ω—é
        local UserInputService = game:GetService("UserInputService")
        local dragging = false
        local dragStart = Vector3.new()
        local startPos = UDim2.new()

        MainFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                dragging = true
                dragStart = Vector3.new(input.Position.X, input.Position.Y, 0)
                startPos = MainFrame.Position
            end
        end)

        MainFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                dragging = false
            end
        end)

        UserInputService.InputChanged:Connect(function(input)
            if dragging and input.UserInputType == Enum.UserInputType.Touch then
                local delta = Vector3.new(input.Position.X, input.Position.Y, 0) - dragStart
                MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
            end
        end)
    end

    -- ========== –°–û–ó–î–ê–ù–ò–ï –°–ö–ê–ù–ï–†–ê –ë–≠–ö–î–û–†–ê ========== --

    local function createBackdoorScanner()
        if game:GetService("CoreGui"):FindFirstChild("BackdoorScanner") then
            game:GetService("CoreGui").BackdoorScanner:Destroy()
        end

        local ScannerGui = Instance.new("ScreenGui")
        ScannerGui.Name = "BackdoorScanner"
        ScannerGui.Parent = game:GetService("CoreGui")
        ScannerGui.ResetOnSpawn = false

        local ScannerFrame = Instance.new("Frame")
        ScannerFrame.Size = UDim2.new(0.25, 0, 0.1, 0)
        ScannerFrame.Position = UDim2.new(0.75, 0, 0.02, 0)
        ScannerFrame.BackgroundColor3 = Color3.fromRGB(10, 10, 10)
        ScannerFrame.BorderColor3 = Color3.fromRGB(100, 100, 100)
        ScannerFrame.BorderSizePixel = 2
        ScannerFrame.Active = true
        ScannerFrame.Parent = ScannerGui

        local Title = Instance.new("TextLabel")
        Title.Size = UDim2.new(1, 0, 0.3, 0)
        Title.Text = "BACKDOOR SCANNER"
        Title.TextColor3 = Color3.fromRGB(255, 255, 0)
        Title.BackgroundTransparency = 1
        Title.Font = Enum.Font.SourceSansBold
        Title.TextSize = 12
        Title.Parent = ScannerFrame

        local StatusText = Instance.new("TextLabel")
        StatusText.Name = "StatusText"
        StatusText.Size = UDim2.new(1, 0, 0.5, 0)
        StatusText.Position = UDim2.new(0, 0, 0.3, 0)
        StatusText.Text = "SCANNING..."
        StatusText.TextColor3 = Color3.fromRGB(255, 255, 255)
        StatusText.BackgroundTransparency = 1
        StatusText.Font = Enum.Font.SourceSansBold
        StatusText.TextSize = 14
        StatusText.Parent = ScannerFrame

        local ScanButton = Instance.new("TextButton")
        ScanButton.Size = UDim2.new(0.8, 0, 0.3, 0)
        ScanButton.Position = UDim2.new(0.1, 0, 0.65, 0)
        ScanButton.Text = "üîç SCAN"
        ScanButton.BackgroundColor3 = Color3.fromRGB(50, 50, 200)
        ScanButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        ScanButton.Font = Enum.Font.SourceSansBold
        ScanButton.TextSize = 12
        ScanButton.Parent = ScannerFrame
        ScanButton.MouseButton1Click:Connect(checkBackdoor)

        -- –°–∏—Å—Ç–µ–º–∞ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏—è —Å–∫–∞–Ω–µ—Ä–∞
        local draggingScanner = false
        local scannerDragStart = Vector3.new()
        local scannerStartPos = UDim2.new()

        ScannerFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                draggingScanner = true
                scannerDragStart = Vector3.new(input.Position.X, input.Position.Y, 0)
                scannerStartPos = ScannerFrame.Position
            end
        end)

        ScannerFrame.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.Touch then
                draggingScanner = false
            end
        end)

        UserInputService.InputChanged:Connect(function(input)
            if draggingScanner and input.UserInputType == Enum.UserInputType.Touch then
                local delta = Vector3.new(input.Position.X, input.Position.Y, 0) - scannerDragStart
                ScannerFrame.Position = UDim2.new(scannerStartPos.X.Scale, scannerStartPos.X.Offset + delta.X, scannerStartPos.Y.Scale, scannerStartPos.Y.Offset + delta.Y)
            end
        end)

        -- –ê–≤—Ç–æ–ø—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∏ —Å–æ–∑–¥–∞–Ω–∏–∏
        checkBackdoor()
    end

    -- –°–æ–∑–¥–∞–µ–º –æ–±–∞ –º–µ–Ω—é
    createMainMenu()
    createBackdoorScanner()

    -- –ê–≤—Ç–æ–≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏–µ
    game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function()
        task.wait(3)
        if not game:GetService("CoreGui"):FindFirstChild("DarkGPT_FE_Menu") then
            createMainMenu()
        end
        if not game:GetService("CoreGui"):FindFirstChild("BackdoorScanner") then
            createBackdoorScanner()
        end
    end)
end
